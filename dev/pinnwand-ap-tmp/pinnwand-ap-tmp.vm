    height: 0; 
    margin: 0 auto;
    border-color: light-yellow;
    border-top: 1px solid black;
}

/* hover effekt fuer karten */
.hover-bg.sticky-note:hover .index-card, .hover-bg.sticky-note:hover .date-on-sticky-note {
